{
  "name": "web3-template",
  "version": "0.1.0",
  "private": true,
  "author": "Liam Dev",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "mac-testnet": "APP_ENV=testnet next dev -p 4000",
    "mac-mainnet": "APP_ENV=mainnet next dev -p 4000",
    "format": "prettier --write .",
    "generate": "next build && next export -o html",
    "postinstall": "husky install",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "typechain": "typechain --out-dir src/constants/typechain --target=ethers-v5 \"src/constants/abi/*/*.json\""
  },
  "dependencies": {
    "@ethersproject/abi": "^5.0.0",
    "@ethersproject/address": "^5.0.0",
    "@ethersproject/bignumber": "^5.0.0",
    "@ethersproject/bytes": "^5.0.0",
    "@ethersproject/constants": "^5.0.0",
    "@ethersproject/contracts": "^5.0.0",
    "@ethersproject/experimental": "^5.0.0",
    "@ethersproject/providers": "^5.0.0",
    "@ethersproject/strings": "^5.0.0",
    "@ethersproject/units": "^5.0.0",
    "@types/humps": "^2.0.2",
    "antd": "^5.4.0",
    "axios": "^1.3.5",
    "ethers": "5.7.2",
    "humps": "^2.0.1",
    "jwt-decode": "^3.1.2",
    "next": "13.3.0",
    "react": "18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "18.2.0",
    "react-toastify": "^9.1.2",
    "sweetalert2": "^11.7.3",
    "sweetalert2-react-content": "5.0.7",
    "swr": "^2.1.2",
    "uuid": "^9.0.0",
    "wagmi": "^0.12.10",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@bonehub/tailwind-dynamic-colors": "^0.0.3",
    "@svgr/webpack": "^7.0.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.5.1",
    "dotenv-flow": "^3.2.0",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "react-icons": "^4.8.0",
    "sass": "^1.61.0",
    "tailwind-merge": "^1.12.0",
    "tailwind-scrollbar": "^3.0.0",
    "tailwindcss": "^3.3.1",
    "tailwindcss-animate": "^1.0.5",
    "typechain": "^8.1.1",
    "typescript": "5.0.4"
  }
}
